    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) 启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/my_PAg_IHRT-no-no-no-no-lru-1core",
            "args": [
				"-warmup_instructions",
				"1000000",
				"-simulation_instructions",
				"10000000",
				"-traces",
				"dpc3_traces/602.gcc_s-734B.champsimtrace.xz"
			],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}